@model System.Collections.Generic.IEnumerable<SportsStore.Domain.Entities.Product>

@{
    ViewBag.Title = "Products";
}
@foreach (var product in Model)
{
    <div>
        <h3>@product.Name
            @product.Description</h3>
        <h4>@product.Price.ToString("C")</h4>
    </div>
}
    
